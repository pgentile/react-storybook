{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "babel": "babel",
    "clean": "rimraf build .loki",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint:js": "eslint --ext .jsx,.js,.ts,.tsx ./src ./e2e-test-*",
    "lint:js:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx ./src ./e2e-test-*",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:css:fix": "stylelint --syntax scss --fix 'src/**/*.scss'",
    "lint": "run-s lint:js lint:css",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:e2e:puppeteer": "jest --runInBand --config=./e2e-test-puppeteer/jest.config.js",
    "test:e2e:webdriverio": "jest --runInBand --config=./e2e-test-webdriverio/jest.config.js",
    "test:e2e:selenium": "jest --runInBand --config=./e2e-test-selenium/jest.config.js",
    "es-check": "es-check es5 'build/storybook/*.js'",
    "build": "run-s lint test:coverage storybook:build es-check",
    "loki:update": "loki update",
    "loki:test": "loki test",
    "reg-cli": "reg-cli .loki/current .loki/reference .loki/difference -R build/reg-cli-report.html -J build/reg-cli-report.json",
    "visual-regression": "run-s --continue-on-error loki:test reg-cli",
    "ci:validate:lint": "run-s lint",
    "ci:validate:test": "run-s test:coverage",
    "ci:build": "run-s storybook:build",
    "ci:post-validate": "run-s es-check"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^1.5.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@xstate/react": "^0.8.1",
    "card-validator": "^6.0.0",
    "core-js": "3",
    "credit-card-type": "^8.1.0",
    "date-fns": "^2.9.0",
    "fast-memoize": "^2.5.1",
    "final-form": "^4.18.7",
    "final-form-focus": "^1.1.2",
    "formik": "^2.1.4",
    "intl-locales-supported": "^1.8.4",
    "lodash-es": "^4.17.8",
    "makecancelable": "^1.1.1",
    "memoize-one": "^5.0.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.8.2",
    "react-cache": "next",
    "react-dom": "^16.8.2",
    "react-final-form": "^6.3.5",
    "react-intl": "^3.12.0",
    "react-jsonschema-form": "^1.8.1",
    "react-redux": "^7.0.1",
    "react-spring": "^8.0.20",
    "react-use": "^13.24.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.1",
    "redux-freeze": "^0.1.5",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.0",
    "xstate": "^4.7.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-storyshots": "^5.3.13",
    "@storybook/addons": "^5.3.12",
    "@storybook/cli": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-intl": "^5.1.18",
    "browserslist": "^4.8.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.4",
    "es-check": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-formatjs": "^1.5.11",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.1.2",
    "file-loader": "^5.0.2",
    "husky": "^4.2.2",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.5",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.0.7",
    "loki": "^0.18.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.1",
    "puppeteer": "^2.1.1",
    "query-string": "^6.10.1",
    "react-test-renderer": "^16.6.3",
    "reg-cli": "^0.16.0",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "style-loader": "^1.1.3",
    "stylelint": "^13.1.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "url-loader": "^3.0.0",
    "webdriverio": "^4.13.2",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-manifest-plugin": "^2.0.4",
    "ws": "^7.2.1"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn install --pure-lockfile && lint-staged && yarn test",
      "pre-push": "yarn install --pure-lockfile && yarn test",
      "post-checkout": "yarn install --pure-lockfile"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --syntax scss --fix"
  }
}
