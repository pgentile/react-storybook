{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf ./build",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint": "eslint --ext .jsx,.js ./src",
    "lint:fix": "eslint --fix --ext .jsx,.js ./src",
    "prettier": "prettier --write src/**/*.{js,jsx}",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:e2e:puppeteer": "jest --runInBand --config=./e2e-test-puppeteer/jest.config.js",
    "test:e2e:webdriverio": "jest --runInBand --config=./e2e-test-webdriverio/jest.config.js",
    "test:e2e:selenium": "jest --config=./e2e-test-selenium/jest.config.js",
    "e2e-mock-pages": "http-server ./e2e-mock-pages -p 10101",
    "build": "npm-run-all lint test test:coverage storybook:build",
    "browserslist": "browserslist",
    "selenium-server": "selenium-server -port 4444"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "babel-polyfill": "^6.26.0",
    "card-validator": "^5.0.0",
    "credit-card-type": "^7.0.0",
    "date-fns": "^1.29.0",
    "fast-memoize": "^2.5.1",
    "formik": "^1.0.3",
    "lodash-es": "^4.17.8",
    "makecancelable": "^1.0.0",
    "memoize-one": "^4.0.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-jsonschema-form": "^1.0.4",
    "react-redux": "^5.0.7",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-freeze": "^0.1.5",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/addon-storyshots": "^3.4.10",
    "@storybook/addons": "^3.4.0",
    "@storybook/cli": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "autoprefixer": "^9.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.0.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browserslist": "^4.0.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "http-server": "^0.11.1",
    "jest": "^23.0.0",
    "jest-extended": "^0.11.0",
    "jest-puppeteer": "^3.3.1",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "puppeteer": "^1.7.0",
    "query-string": "^6.1.0",
    "react-test-renderer": "^16.3.2",
    "regenerator-runtime": "^0.12.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webdriverio": "^4.13.2",
    "webpack": "^4.5.0",
    "ws": "^6.0.0"
  },
  "browserslist": [
    "last 3 chrome versions",
    "last 3 firefox versions",
    "last 3 safari versions",
    "last 3 edge versions",
    "last 1 ie version",
    "last 3 ChromeAndroid versions",
    "last 3 Android versions",
    "last 3 iOS versions",
    ">= 0.5% in FR",
    ">= 0.5% in CH",
    ">= 0.5% in BE",
    ">= 0.5% in DE",
    ">= 0.5% in GB"
  ]
}
