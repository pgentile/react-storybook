{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "babel": "babel",
    "clean": "rimraf build .loki",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint:js": "eslint --ext .jsx,.js ./src ./e2e-test-*",
    "lint:js:fix": "eslint --fix --ext .jsx,.js ./src ./e2e-test-*",
    "prettier": "prettier --write 'src/**/*.{js,jsx}'",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:css:fix": "stylelint --fix 'src/**/*.scss'",
    "lint": "run-s lint:js lint:css",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:e2e:puppeteer": "jest --runInBand --config=./e2e-test-puppeteer/jest.config.js",
    "test:e2e:webdriverio": "jest --runInBand --config=./e2e-test-webdriverio/jest.config.js",
    "test:e2e:selenium": "jest --runInBand --config=./e2e-test-selenium/jest.config.js",
    "es-check": "es-check es5 'build/storybook/*.js'",
    "build": "run-s lint test:coverage storybook:build es-check",
    "loki:update": "loki update",
    "loki:test": "loki test",
    "reg-cli": "reg-cli .loki/current .loki/reference .loki/difference -R build/reg-cli-report.html -J build/reg-cli-report.json",
    "visual-regression": "run-s --continue-on-error loki:test reg-cli",
    "ci:validate:lint": "run-s lint",
    "ci:validate:test": "run-s test:coverage",
    "ci:build": "run-s storybook:build",
    "ci:post-validate": "run-s es-check"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "^0.1.6",
    "@xstate/react": "^0.7.0",
    "card-validator": "^6.0.0",
    "core-js": "2",
    "credit-card-type": "^8.1.0",
    "date-fns": "^1.29.0",
    "fast-memoize": "^2.5.1",
    "formik": "^1.3.2",
    "lodash-es": "^4.17.8",
    "makecancelable": "^1.1.1",
    "memoize-one": "^5.0.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.8.2",
    "react-cache": "next",
    "react-dom": "^16.8.2",
    "react-jsonschema-form": "^1.2.0",
    "react-redux": "^7.0.1",
    "react-spring": "^8.0.20",
    "react-use": "^12.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-freeze": "^0.1.5",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.0",
    "xstate": "^4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addon-storyshots": "^5.2.3",
    "@storybook/addons": "^5.2.4",
    "@storybook/cli": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "autoprefixer": "^9.6.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-require-context-hook": "^1.0.0",
    "browserslist": "^4.5.6",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.4",
    "es-check": "^5.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-compat": "^3.0.1",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "file-loader": "^4.2.0",
    "husky": "^3.0.8",
    "jest": "^24.0.0",
    "jest-extended": "^0.11.0",
    "jest-puppeteer": "^4.0.0",
    "loki": "^0.15.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.1",
    "puppeteer": "^1.14.0",
    "query-string": "^6.1.0",
    "react-test-renderer": "^16.6.3",
    "reg-cli": "^0.14.1",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-scss": "^3.6.1",
    "url-loader": "^2.2.0",
    "webdriverio": "^4.13.2",
    "webpack": "^4.31.0",
    "webpack-manifest-plugin": "^2.0.4",
    "ws": "^7.0.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn lint && yarn test",
      "pre-push": "yarn test",
      "post-checkout": "yarn install --pure-lockfile"
    }
  }
}
