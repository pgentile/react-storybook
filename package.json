{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf build node_modules/.cache",
    "start": "run-s storybook",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint:js": "eslint --ext .jsx,.js,.ts,.tsx --ignore-pattern '**/*.d.ts' ./src",
    "lint:js:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx --ignore-pattern '**/*.d.ts' ./src",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:css:fix": "stylelint --syntax scss --fix 'src/**/*.scss'",
    "lint": "run-s lint:js lint:css",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest",
    "test:strict": "cross-env NODE_ICU_DATA=./node_modules/full-icu node --unhandled-rejections=strict ./node_modules/.bin/jest --runInBand --verbose --errorOnDeprecated --bail",
    "validate": "run-s lint test",
    "test:update": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest -u",
    "test:coverage": "jest --coverage",
    "es-check": "es-check es5 'build/storybook/*.js'",
    "build": "run-s lint test:coverage storybook:build es-check",
    "ci:validate:lint": "run-s lint",
    "ci:validate:test": "run-s test:coverage",
    "ci:build": "run-s storybook:build",
    "ci:post-validate": "run-s es-check",
    "babel:development": "cross-env NODE_ENV=development babel",
    "babel:production": "cross-env NODE_ENV=production babel",
    "babel:test": "cross-env NODE_ENV=test babel"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^1.5.9",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@xstate/react": "^0.8.1",
    "card-validator": "^6.0.0",
    "core-js": "3",
    "credit-card-type": "^8.1.0",
    "date-fns": "^2.14.0",
    "emotion": "^10.0.27",
    "fast-memoize": "^2.5.2",
    "final-form": "^4.20.0",
    "final-form-focus": "^1.1.2",
    "intl-locales-supported": "^1.8.10",
    "lodash-es": "^4.17.8",
    "makecancelable": "^1.1.1",
    "memoize-one": "^5.0.0",
    "polished": "^3.6.4",
    "pretty": "^2.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-cache": "next",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.4.0",
    "react-intl": "^4.6.5",
    "react-jsonschema-form": "^1.8.1",
    "react-redux": "^7.2.0",
    "react-spring": "^8.0.20",
    "react-use": "^15.1.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.1",
    "redux-freeze": "^0.1.5",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.0",
    "xstate": "^4.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/cli": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.1.0",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-react-intl": "^7.5.17",
    "browserslist": "^4.12.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "es-check": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-compat": "^3.7.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-formatjs": "^2.3.10",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "full-icu": "^1.3.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-emotion": "^10.0.32",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.2.9",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "query-string": "^6.12.1",
    "react-is": "^16.8.0",
    "react-test-renderer": "^16.13.1",
    "recursive-readdir": "^2.2.2",
    "reg-cli": "^0.16.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "storybook-addon-i18n": "^5.1.13",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.17.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn install --pure-lockfile && lint-staged && yarn test",
      "pre-push": "yarn install --pure-lockfile && yarn test",
      "post-checkout": "yarn install --pure-lockfile"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --ignore-pattern '**/*.d.ts' --fix",
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --syntax scss --fix"
  }
}
