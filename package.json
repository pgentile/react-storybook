{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf build node_modules/.cache",
    "start": "run-s storybook",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint:js": "eslint --ext .jsx,.js,.ts,.tsx --ignore-pattern '**/*.d.ts' ./src",
    "lint:js:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx --ignore-pattern '**/*.d.ts' ./src",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:css:fix": "stylelint --syntax scss --fix 'src/**/*.scss'",
    "lint:yarnlock": "yarn-deduplicate --list",
    "lint": "run-s lint:yarnlock lint:js lint:css",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --runInBand",
    "test:strict": "cross-env NODE_ICU_DATA=./node_modules/full-icu node --unhandled-rejections=strict ./node_modules/.bin/jest --runInBand --verbose --errorOnDeprecated --bail",
    "validate": "run-s lint test",
    "test:update": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --runInBand -u",
    "test:coverage": "jest --runInBand --coverage",
    "es-check": "es-check es5 'build/storybook/*.js'",
    "build": "run-s lint test:coverage storybook:build es-check",
    "ci:validate:lint": "run-s lint",
    "ci:validate:test": "run-s test:coverage",
    "ci:build": "run-s storybook:build",
    "ci:post-validate": "run-s es-check",
    "babel:development": "cross-env NODE_ENV=development babel",
    "babel:production": "cross-env NODE_ENV=production babel",
    "babel:test": "cross-env NODE_ENV=test babel",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@emotion/css": "11.0.0",
    "@formatjs/intl-datetimeformat": "3.1.0",
    "@formatjs/intl-getcanonicallocales": "1.5.2",
    "@formatjs/intl-listformat": "5.0.1",
    "@formatjs/intl-locale": "2.4.8",
    "@formatjs/intl-numberformat": "6.0.0",
    "@formatjs/intl-pluralrules": "4.0.0",
    "@formatjs/intl-relativetimeformat": "8.0.0",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.13",
    "@xstate/react": "1.0.3",
    "card-validator": "8.1.0",
    "core-js": "3.8.0",
    "credit-card-type": "9.1.0",
    "date-fns": "2.16.1",
    "fast-memoize": "2.5.2",
    "final-form": "4.20.1",
    "final-form-focus": "1.1.2",
    "libphonenumber-js": "1.9.4",
    "lodash-es": "4.17.15",
    "makecancelable": "1.1.1",
    "md5": "2.3.0",
    "memoize-one": "5.1.1",
    "polished": "4.0.5",
    "pretty": "2.0.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-cache": "next",
    "react-dom": "16.14.0",
    "react-final-form": "6.5.2",
    "react-intl": "5.10.6",
    "react-jsonschema-form": "1.8.1",
    "react-redux": "7.2.2",
    "react-spring": "8.0.27",
    "react-use": "15.3.4",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-freeze": "0.1.7",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
    "reselect": "4.0.0",
    "resize-observer-polyfill": "1.5.1",
    "xstate": "4.14.1"
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-proposal-private-methods": "7.12.1",
    "@babel/plugin-transform-react-jsx": "7.12.7",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@emotion/babel-plugin": "11.0.0",
    "@emotion/eslint-plugin": "11.0.0",
    "@emotion/jest": "11.1.0",
    "@formatjs/cli": "2.13.15",
    "@storybook/addon-a11y": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-storyshots": "6.0.28",
    "@storybook/cli": "6.0.28",
    "@storybook/react": "6.0.28",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "autoprefixer": "10.0.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-macros": "3.0.0",
    "babel-plugin-react-intl": "8.2.21",
    "browserslist": "4.14.7",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.2",
    "css-loader": "5.0.1",
    "cssnano": "4.1.10",
    "es-check": "5.1.4",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-formatjs": "2.9.10",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "full-icu": "1.3.1",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "lint-staged": "10.5.2",
    "node-sass": "5.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.1.10",
    "postcss-loader": "4.1.0",
    "prettier": "2.2.1",
    "query-string": "6.13.7",
    "react-is": "16.13.1",
    "react-test-renderer": "16.14.0",
    "recursive-readdir": "2.2.2",
    "reg-cli": "0.16.3",
    "rimraf": "3.0.2",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "stylelint": "13.8.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-no-unsupported-browser-features": "4.1.4",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "webpack-bundle-analyzer": "4.1.0",
    "yarn-deduplicate": "3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint-staged test",
      "pre-push": "run-s test"
    }
  },
  "lint-staged": {
    "yarn.lock": "yarn-deduplicate --list",
    "*.{js,jsx,ts,tsx}": "eslint --ignore-pattern '**/*.d.ts' --fix",
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --syntax scss --fix"
  }
}
