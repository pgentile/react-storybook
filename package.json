{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf build node_modules/.cache",
    "start": "run-s storybook",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint:js": "eslint --ext .jsx,.js,.ts,.tsx ./src",
    "lint:js:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx ./src",
    "lint:js:config": "eslint --ext .jsx,.js,.ts,.tsx --print-config",
    "lint:ts": "tsc --noEmit",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:css:fix": "stylelint --syntax scss --fix 'src/**/*.scss'",
    "lint:yarnlock": "yarn-deduplicate --list",
    "lint": "run-s lint:yarnlock lint:js lint:ts lint:css",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --runInBand",
    "test:strict": "cross-env NODE_ICU_DATA=./node_modules/full-icu node --unhandled-rejections=strict ./node_modules/.bin/jest --runInBand --verbose --errorOnDeprecated --bail",
    "validate": "run-s lint test",
    "test:update": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --runInBand -u",
    "test:coverage": "jest --runInBand --coverage",
    "build": "run-s lint test:coverage storybook:build",
    "ci:validate:lint": "run-s lint",
    "ci:validate:test": "run-s test:coverage",
    "ci:build": "run-s storybook:build",
    "babel:development": "cross-env NODE_ENV=development babel",
    "babel:production": "cross-env NODE_ENV=production babel",
    "babel:test": "cross-env NODE_ENV=test babel",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@emotion/css": "11.5.0",
    "@formatjs/intl-datetimeformat": "4.2.5",
    "@formatjs/intl-getcanonicallocales": "1.8.0",
    "@formatjs/intl-listformat": "6.3.5",
    "@formatjs/intl-locale": "2.4.40",
    "@formatjs/intl-numberformat": "7.2.5",
    "@formatjs/intl-pluralrules": "4.1.5",
    "@formatjs/intl-relativetimeformat": "9.3.2",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@xstate/react": "1.6.2",
    "card-validator": "8.1.1",
    "core-js": "3.19.1",
    "credit-card-type": "9.1.0",
    "date-fns": "2.26.0",
    "fast-memoize": "2.5.2",
    "final-form": "4.20.4",
    "final-form-focus": "1.1.2",
    "libphonenumber-js": "1.9.43",
    "lodash-es": "4.17.21",
    "makecancelable": "1.1.1",
    "md5": "2.3.0",
    "memoize-one": "6.0.0",
    "polished": "4.1.3",
    "pretty": "2.0.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.7",
    "react-intl": "5.21.2",
    "react-jsonschema-form": "1.8.1",
    "react-redux": "7.2.6",
    "react-spring": "9.3.1",
    "react-use": "17.3.1",
    "redux": "4.1.2",
    "redux-actions": "2.6.5",
    "redux-freeze": "0.1.7",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "2.4.0",
    "regenerator-runtime": "0.13.9",
    "reselect": "4.1.4",
    "resize-observer-polyfill": "1.5.1",
    "xstate": "4.26.0"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/eslint-plugin": "11.5.0",
    "@emotion/jest": "11.6.0",
    "@formatjs/cli": "4.4.0",
    "@storybook/addon-a11y": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-storyshots": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/cli": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/react": "6.3.12",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@tsconfig/recommended": "1.0.1",
    "@types/lodash": "4.14.177",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "autoprefixer": "10.4.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "babel-plugin-formatjs": "10.3.11",
    "babel-plugin-macros": "3.1.0",
    "browserslist": "4.18.1",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.5.1",
    "cssnano": "5.0.11",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-compat": "3.13.0",
    "eslint-plugin-formatjs": "2.18.0",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "full-icu": "1.4.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-extended": "1.1.0",
    "lint-staged": "11.2.6",
    "node-sass": "6.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.11",
    "postcss-loader": "6.2.0",
    "prettier": "2.4.1",
    "query-string": "7.0.1",
    "react-is": "17.0.2",
    "react-test-renderer": "17.0.2",
    "recursive-readdir": "2.2.2",
    "reg-cli": "0.17.4",
    "rimraf": "3.0.2",
    "sass-loader": "12.3.0",
    "style-loader": "3.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-no-unsupported-browser-features": "5.0.2",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.21.0",
    "typescript": "4.5.2",
    "webpack": "5.64.2",
    "webpack-bundle-analyzer": "4.5.0",
    "yarn-deduplicate": "3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint-staged test",
      "pre-push": "run-s test"
    }
  },
  "lint-staged": {
    "yarn.lock": "yarn-deduplicate --list",
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --syntax scss --fix"
  }
}
