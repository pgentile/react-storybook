{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf build node_modules/.cache",
    "start": "run-s storybook",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint:js": "eslint --ext .jsx,.js,.ts,.tsx --ignore-pattern '**/*.d.ts' ./src",
    "lint:js:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx --ignore-pattern '**/*.d.ts' ./src",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:css:fix": "stylelint --syntax scss --fix 'src/**/*.scss'",
    "lint": "run-s lint:js lint:css",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --runInBand",
    "test:strict": "cross-env NODE_ICU_DATA=./node_modules/full-icu node --unhandled-rejections=strict ./node_modules/.bin/jest --runInBand --verbose --errorOnDeprecated --bail",
    "validate": "run-s lint test",
    "test:update": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --runInBand -u",
    "test:coverage": "jest --runInBand --coverage",
    "es-check": "es-check es5 'build/storybook/*.js'",
    "build": "run-s lint test:coverage storybook:build es-check",
    "ci:validate:lint": "run-s lint",
    "ci:validate:test": "run-s test:coverage",
    "ci:build": "run-s storybook:build",
    "ci:post-validate": "run-s es-check",
    "babel:development": "cross-env NODE_ENV=development babel",
    "babel:production": "cross-env NODE_ENV=production babel",
    "babel:test": "cross-env NODE_ENV=test babel"
  },
  "dependencies": {
    "@formatjs/intl-getcanonicallocales": "1.4.3",
    "@formatjs/intl-numberformat": "5.6.0",
    "@formatjs/intl-pluralrules": "3.4.6",
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-brands-svg-icons": "5.14.0",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@xstate/react": "0.8.1",
    "card-validator": "8.1.0",
    "core-js": "3.6.5",
    "credit-card-type": "9.0.1",
    "date-fns": "2.16.1",
    "emotion": "10.0.27",
    "fast-memoize": "2.5.2",
    "final-form": "4.20.1",
    "final-form-focus": "1.1.2",
    "intl-locales-supported": "1.8.12",
    "libphonenumber-js": "1.7.57",
    "lodash-es": "4.17.15",
    "makecancelable": "1.1.1",
    "md5": "2.3.0",
    "memoize-one": "5.1.1",
    "polished": "3.6.6",
    "pretty": "2.0.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-cache": "next",
    "react-dom": "16.13.1",
    "react-final-form": "6.5.1",
    "react-intl": "5.8.0",
    "react-jsonschema-form": "1.8.1",
    "react-redux": "7.2.1",
    "react-spring": "8.0.27",
    "react-use": "15.3.4",
    "recompose": "0.30.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-freeze": "0.1.7",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
    "reselect": "4.0.0",
    "resize-observer-polyfill": "1.5.1",
    "xstate": "4.13.0"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@babel/plugin-proposal-private-methods": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@formatjs/cli": "2.9.0",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-controls": "6.0.21",
    "@storybook/addon-docs": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/addon-storyshots": "6.0.21",
    "@storybook/addon-viewport": "6.0.21",
    "@storybook/addons": "6.0.21",
    "@storybook/cli": "6.0.21",
    "@storybook/react": "6.0.21",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.2",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-react-intl": "8.2.0",
    "browserslist": "4.14.1",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.2",
    "css-loader": "4.3.0",
    "cssnano": "4.1.10",
    "es-check": "5.1.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-formatjs": "2.7.8",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "file-loader": "6.1.0",
    "full-icu": "1.3.1",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-emotion": "10.0.32",
    "jest-extended": "0.11.5",
    "lint-staged": "10.3.0",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "prettier": "2.1.1",
    "query-string": "6.13.1",
    "react-is": "16.13.1",
    "react-test-renderer": "16.13.1",
    "recursive-readdir": "2.2.2",
    "reg-cli": "0.16.1",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.2",
    "storybook-addon-i18n": "5.1.13",
    "style-loader": "1.2.1",
    "stylelint": "13.7.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-no-unsupported-browser-features": "4.0.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-manifest-plugin": "2.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn install --pure-lockfile && lint-staged && yarn test",
      "pre-push": "yarn install --pure-lockfile && yarn test",
      "post-checkout": "yarn install --pure-lockfile"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --ignore-pattern '**/*.d.ts' --fix",
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --syntax scss --fix"
  }
}
