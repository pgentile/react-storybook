{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "babel": "babel",
    "clean": "rimraf build .loki",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint:js": "eslint --ext .jsx,.js ./src",
    "lint:js:fix": "eslint --fix --ext .jsx,.js ./src",
    "prettier": "prettier --write 'src/**/*.{js,jsx}'",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:css:fix": "stylelint --fix 'src/**/*.scss'",
    "lint": "npm-run-all lint:js lint:css",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:e2e:puppeteer": "jest --runInBand --config=./e2e-test-puppeteer/jest.config.js",
    "test:e2e:webdriverio": "jest --runInBand --config=./e2e-test-webdriverio/jest.config.js",
    "test:e2e:selenium": "jest --config=./e2e-test-selenium/jest.config.js",
    "e2e-mock-pages": "http-server ./e2e-mock-pages -p 10101",
    "es-check": "es-check es5 'build/storybook/*.js'",
    "build": "npm-run-all lint test:coverage storybook:build es-check",
    "browserslist": "browserslist",
    "selenium-server": "selenium-server -port 4444",
    "loki:update": "loki update",
    "loki:test": "loki test",
    "reg-cli": "reg-cli .loki/current .loki/reference .loki/difference -R build/reg-cli-report.html -J build/reg-cli-report.json",
    "visual-regression": "npm-run-all --continue-on-error loki:test reg-cli"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "card-validator": "^6.0.0",
    "credit-card-type": "^8.1.0",
    "date-fns": "^1.29.0",
    "fast-memoize": "^2.5.1",
    "formik": "^1.3.2",
    "lodash-es": "^4.17.8",
    "makecancelable": "^1.0.0",
    "memoize-one": "^5.0.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.8.2",
    "react-cache": "next",
    "react-dom": "^16.8.2",
    "react-jsonschema-form": "^1.2.0",
    "react-redux": "^6.0.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-freeze": "^0.1.5",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.0",
    "xstate": "^4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addon-storyshots": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/cli": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "autoprefixer": "^9.4.6",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-require-context-hook": "^1.0.0",
    "browserslist": "^4.4.1",
    "css-loader": "^2.0.0",
    "cssnano": "^4.1.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "es-check": "^5.0.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "file-loader": "^3.0.1",
    "http-server": "^0.11.1",
    "jest": "^24.0.0",
    "jest-extended": "^0.11.0",
    "jest-puppeteer": "^4.0.0",
    "loki": "^0.14.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.1",
    "puppeteer": "^1.7.0",
    "query-string": "^6.1.0",
    "react-test-renderer": "^16.6.3",
    "reg-cli": "^0.14.1",
    "regenerator-runtime": "^0.13.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-scss": "^3.5.1",
    "url-loader": "^1.1.1",
    "webdriverio": "^4.13.2",
    "webpack": "^4.29.0",
    "webpack-manifest-plugin": "^2.0.4",
    "ws": "^6.1.3"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768
      }
    }
  }
}
