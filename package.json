{
  "name": "react-storybook",
  "version": "1.0.0",
  "author": "Pierre Gentile",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf build node_modules/.cache",
    "start": "run-s storybook",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook --output-dir ./build/storybook",
    "lint:js": "eslint --ext .jsx,.js,.ts,.tsx ./src",
    "lint:js:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx ./src",
    "lint:js:config": "eslint --ext .jsx,.js,.ts,.tsx --print-config",
    "lint:ts": "tsc --noEmit",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "lint:css": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:css:fix": "stylelint --syntax scss --fix 'src/**/*.scss'",
    "lint:yarnlock": "yarn-deduplicate --list",
    "lint": "run-s lint:yarnlock lint:js lint:ts lint:css",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --runInBand",
    "test:strict": "cross-env NODE_ICU_DATA=./node_modules/full-icu node --unhandled-rejections=strict ./node_modules/.bin/jest --runInBand --verbose --errorOnDeprecated --bail",
    "validate": "run-s lint test",
    "test:update": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --runInBand -u",
    "test:coverage": "jest --runInBand --coverage",
    "es-check": "es-check es5 'build/storybook/*.js'",
    "build": "run-s lint test:coverage storybook:build es-check",
    "ci:validate:lint": "run-s lint",
    "ci:validate:test": "run-s test:coverage",
    "ci:build": "run-s storybook:build",
    "ci:post-validate": "run-s es-check",
    "babel:development": "cross-env NODE_ENV=development babel",
    "babel:production": "cross-env NODE_ENV=production babel",
    "babel:test": "cross-env NODE_ENV=test babel",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@emotion/css": "11.1.3",
    "@formatjs/intl-datetimeformat": "3.2.12",
    "@formatjs/intl-getcanonicallocales": "1.5.6",
    "@formatjs/intl-listformat": "5.0.10",
    "@formatjs/intl-locale": "2.4.19",
    "@formatjs/intl-numberformat": "6.2.2",
    "@formatjs/intl-pluralrules": "4.0.11",
    "@formatjs/intl-relativetimeformat": "8.1.2",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@xstate/react": "1.3.1",
    "card-validator": "8.1.0",
    "core-js": "3.9.1",
    "credit-card-type": "9.1.0",
    "date-fns": "2.19.0",
    "fast-memoize": "2.5.2",
    "final-form": "4.20.2",
    "final-form-focus": "1.1.2",
    "libphonenumber-js": "1.9.13",
    "lodash-es": "4.17.21",
    "makecancelable": "1.1.1",
    "md5": "2.3.0",
    "memoize-one": "5.1.1",
    "polished": "4.1.1",
    "pretty": "2.0.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-final-form": "6.5.2",
    "react-intl": "5.13.2",
    "react-jsonschema-form": "1.8.1",
    "react-redux": "7.2.2",
    "react-spring": "8.0.27",
    "react-use": "17.2.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-freeze": "0.1.7",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
    "reselect": "4.0.0",
    "resize-observer-polyfill": "1.5.1",
    "xstate": "4.16.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/plugin-transform-react-jsx": "7.12.17",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@emotion/babel-plugin": "11.2.0",
    "@emotion/eslint-plugin": "11.2.0",
    "@emotion/jest": "11.2.1",
    "@formatjs/cli": "3.1.6",
    "@storybook/addon-a11y": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-storyshots": "6.1.21",
    "@storybook/cli": "6.1.21",
    "@storybook/react": "6.1.21",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@tsconfig/recommended": "1.0.1",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "autoprefixer": "10.2.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-formatjs": "9.2.4",
    "babel-plugin-macros": "3.0.1",
    "browserslist": "4.16.3",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.3",
    "css-loader": "5.1.2",
    "cssnano": "4.1.10",
    "es-check": "5.2.3",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-formatjs": "2.12.7",
    "eslint-plugin-jest": "24.2.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "full-icu": "1.3.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "lint-staged": "10.5.4",
    "node-sass": "5.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.8",
    "postcss-loader": "4.2.0",
    "prettier": "2.2.1",
    "query-string": "6.14.1",
    "react-is": "17.0.1",
    "react-test-renderer": "17.0.1",
    "recursive-readdir": "2.2.2",
    "reg-cli": "0.17.0",
    "rimraf": "3.0.2",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "stylelint": "13.12.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-no-unsupported-browser-features": "4.1.4",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.19.0",
    "typescript": "4.2.3",
    "webpack-bundle-analyzer": "4.4.0",
    "yarn-deduplicate": "3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint-staged test",
      "pre-push": "run-s test"
    }
  },
  "lint-staged": {
    "yarn.lock": "yarn-deduplicate --list",
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --syntax scss --fix"
  }
}
